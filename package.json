{
  "name": "@nordicsemiconductor/public-parameter-registry",
  "version": "0.0.0-development",
  "description": "Public parameter registry developed using AWS CDK in TypeScript",
  "type": "module",
  "scripts": {
    "test": "jest --passWithNoTests",
    "test:e2e": "npx tsx ./feature-runner/run-features.ts | npx tsx ./feature-runner/console-reporter.ts",
    "prepare": "husky install && check-node-version --package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/public-parameter-registry-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/public-parameter-registry-aws-js/issues"
  },
  "homepage": "https://github.com/bifravst/public-parameter-registry-aws-js#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "aws",
    "cdk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.13",
    "@nordicsemiconductor/bdd-markdown": "3.5.39",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "13.0.1",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.42",
    "@swc/jest": "0.2.24",
    "@types/aws-lambda": "8.10.114",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.0",
    "@types/yazl": "2.4.2",
    "aws-cdk-lib": "2.71.0",
    "cdk": "2.71.0",
    "chalk": "5.2.0",
    "check-node-version": "4.2.1",
    "constructs": "10.1.295",
    "eslint-plugin-unicorn": "46.0.0",
    "glob": "9.3.2",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "tsx": "3.12.6",
    "typescript": "5.0.2",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "volta": {
    "node": "18.14.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.301.0",
    "@aws-sdk/client-ssm": "3.301.0",
    "@nordicsemiconductor/from-env": "2.0.0",
    "@sinclair/typebox": "0.26.8",
    "ajv": "8.12.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  }
}
